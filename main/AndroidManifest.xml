<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dude.quizscene">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.VIBRATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/horrorlogo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/MYFUKINGTHEME">
        <activity android:name=".start">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Horror" />
        <activity android:name=".Sports" />
        <activity android:name=".Comedy" />
        <activity android:name=".Action" />
        <activity android:name=".info" />
        <activity android:name=".death" />
        <activity android:name=".Trailer" />
        <activity android:name=".MainActivity" />
        <activity android:name=".Credits" />
        <activity android:name=".Win" />
        <activity
            android:name=".Audio"
            android:label="@string/title_activity_audio"
            android:theme="@style/MYFUKINGTHEME"></activity>
    </application>

</manifest>